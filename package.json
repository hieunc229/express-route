{
  "name": "rumbo-template",
  "version": "0.6.0",
  "description": "Build fullstack app with Express and React",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run clean",
    "start": "NODE_OPTIONS=--max-old-space-size=8094 HOST=0.0.0.0 nodemon -r tsconfig-paths/register src/dev.ts",
    "clean": "rm -rf dist build __rumbo && npm run clean-bundle",
    "clean-bundle": "rm -rf bundle/server bundle/public bundle/__rumbo bundle/dist bundle/*.log && npm run clean-extra",
    "clean-extra": "echo 'add your clean script here'",
    "build": "npm run bundle-clientssr && npm run copy-assets && npm run bundle-client && tsc && npm run bundle-server",
    "prebuild": "yarn clean",
    "postbuild": "cp -r dist/ bundle/public/ && npm run postbuild-extra",
    "postbuild-extra": "echo 'add your postbuild script here'",
    "copy-assets": "copyfiles src/**/*.css build/",
    "bundle-clientssr": "ts-node scripts/bundle-clientssr.ts",
    "bundle-client": "node scripts/bundle-client.js",
    "bundle-server": "node --max-old-space-size=4096 scripts/bundle-server.js"
  },
  "author": "Hieu Nguyen",
  "license": "AGPL-3.0",
  "dependencies": {
    "easier-class": "^0.0.32",
    "express": "^4.18.2",
    "express-useragent": "^1.0.15",
    "go-promise": "^0.0.3",
    "iconsax-react": "^0.0.8",
    "ignore-styles": "^5.0.1",
    "nanoid": "3",
    "node-fetch": "2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.15.0",
    "rumbo": "^0.6.61"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-flow": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@hot-loader/react-dom": "^17.0.2",
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/webpack-hot-middleware": "^2.25.6",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "chalk": "4",
    "css-loader": "^6.8.1",
    "esbuild-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.1",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.1",
    "react-hot-loader": "^4.13.1",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.4",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.9.0",
    "workbox-webpack-plugin": "^7.0.0"
  }
}
